{
    "name": "codeat3/blade-elusive-icons",
    "description": "A package to easily make use of Elusive Icons in your Laravel Blade views.",
    "keywords": ["Blade", "Elusive", "Laravel"],
    "homepage": "https://github.com/codeat3/blade-elusive-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Swapnil Sarwe"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "codeat3/phpcs-styles": "dev-main",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codeat3\\BladeElusiveIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codeat3\\BladeElusiveIcons\\BladeElusiveIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix"
    }
}
